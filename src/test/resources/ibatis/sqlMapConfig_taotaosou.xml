<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMapConfig      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map Config 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-config-2.dtd">
<sqlMapConfig>
	<!-- 配置缓存机制，使用延迟加载 -->
	<settings cacheModelsEnabled="true" enhancementEnabled="true"
		lazyLoadingEnabled="true" errorTracingEnabled="true" maxRequests="512"
		maxSessions="128" maxTransactions="50" useStatementNamespaces="true" />
	<sqlMap resource="ibatis/mapping/ChooseDotProduct.xml" />
	<sqlMap resource="ibatis/mapping/ChooseProduct.xml" />
	<sqlMap resource="ibatis/mapping/MessageConfig.xml" />
	<sqlMap resource="ibatis/mapping/MessageContent.xml" />
	<sqlMap resource="ibatis/mapping/MessageInfo.xml" />
	
</sqlMapConfig>